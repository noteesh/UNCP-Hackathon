# Copy this file to .env and fill in your values.
# .env is gitignored â€” never commit real keys.

# MongoDB Atlas connection string
# Format: mongodb+srv://<user>:<password>@<cluster>.mongodb.net/?retryWrites=true&w=majority
MONGODB_URI=mongodb+srv://YOUR_USER:YOUR_PASSWORD@YOUR_CLUSTER.mongodb.net/?retryWrites=true&w=majority

# Google Gemini API key
GEMINI_API_KEY=your_gemini_api_key_here

# ElevenLabs API key
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Solana keypair (path to a local keypair JSON file, or leave blank to skip)
SOLANA_KEYPAIR_PATH=
